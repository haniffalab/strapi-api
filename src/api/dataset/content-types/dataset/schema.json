{
  "kind": "collectionType",
  "collectionName": "datasets",
  "info": {
    "singularName": "dataset",
    "pluralName": "datasets",
    "displayName": "Dataset",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "datafile": {
      "type": "relation",
      "relation": "morphOne",
      "target": "plugin::data-import.datafile",
      "morphBy": "related",
      "private": true,
      "configurable": false
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "study": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::study.study",
      "inversedBy": "datasets"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "scRNA-seq",
        "CITE-seq",
        "Smart-seq2",
        "VDJ",
        "Visium",
        "Xenium",
        "MERFISH"
      ]
    },
    "uid": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "metadata": {
      "type": "json"
    },
    "experiments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experiment.experiment",
      "inversedBy": "datasets"
    },
    "data": {
      "type": "component",
      "repeatable": true,
      "component": "data.file"
    },
    "tissues": {
      "type": "customField",
      "customField": "plugin::ebi-ols.ontology-term",
      "options": {
        "ontology": "uberon"
      }
    },
    "organisms": {
      "type": "customField",
      "customField": "plugin::ebi-ols.ontology-term",
      "options": {
        "ontology": "uberon"
      }
    },
    "assays": {
      "type": "customField",
      "customField": "plugin::ebi-ols.ontology-term",
      "options": {
        "ontology": "cl"
      }
    },
    "count": {
      "type": "integer"
    }
  }
}
