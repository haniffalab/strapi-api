{
  "kind": "collectionType",
  "collectionName": "experiments",
  "info": {
    "singularName": "experiment",
    "pluralName": "experiments",
    "displayName": "Experiment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "datafile": {
      "type": "relation",
      "relation": "morphOne",
      "target": "plugin::data-import.datafile",
      "morphBy": "related",
      "private": true,
      "configurable": false
    },
    "experimental_factor": {
      "type": "customField",
      "options": {
        "ontology": "efo"
      },
      "customField": "plugin::ebi-ols.ontology-term"
    },
    "date": {
      "type": "date"
    },
    "experiment_id": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "assay": {
      "type": "enumeration",
      "enum": [
        "Drop-Seq",
        "Smart-Seq",
        "Smart-Seq2",
        "CITE-seq",
        "CEL-Seq",
        "CEL-Seq2",
        "Fluidigm_C1",
        "Seq-well",
        "library 10x_3'_v1",
        "library 10x_3'_v2",
        "library 10x_3'_v3",
        "library 10x_3'_NG3.1",
        "library 10x_5'_v1",
        "library 10x_5'_v1.1",
        "library 10x_5'_v2",
        "library 10x_5'_NG",
        "library 10x_ATAC",
        "Visium"
      ]
    },
    "contributors": {
      "displayName": "people",
      "type": "component",
      "repeatable": true,
      "component": "contributions.people"
    },
    "protocols": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::protocol.protocol"
    },
    "data_archives": {
      "displayName": "file",
      "type": "component",
      "repeatable": true,
      "component": "data.file"
    },
    "uid": {
      "type": "uid",
      "targetField": "experiment_id",
      "required": true
    },
    "datasets": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::dataset.dataset",
      "mappedBy": "experiments"
    },
    "study": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::study.study",
      "inversedBy": "experiments"
    },
    "samples": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sample.sample",
      "mappedBy": "experiments"
    }
  }
}
