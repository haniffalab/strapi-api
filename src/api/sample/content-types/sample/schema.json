{
  "kind": "collectionType",
  "collectionName": "samples",
  "info": {
    "singularName": "sample",
    "pluralName": "samples",
    "displayName": "Sample",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "datafile": {
      "type": "relation",
      "relation": "morphOne",
      "target": "plugin::data-import.datafile",
      "morphBy": "related",
      "private": true,
      "configurable": false
    },
    "sample_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "donor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::donor.donor"
    },
    "tissue": {
      "type": "customField",
      "options": {
        "ontology": "uberon"
      },
      "customField": "plugin::ebi-ols.ontology-term"
    },
    "organism": {
      "type": "customField",
      "options": {
        "ontology": "uberon"
      },
      "customField": "plugin::ebi-ols.ontology-term"
    },
    "contributors": {
      "type": "component",
      "repeatable": true,
      "component": "contributions.people"
    },
    "uid": {
      "type": "uid",
      "targetField": "sample_id",
      "required": true
    },
    "experiments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experiment.experiment",
      "inversedBy": "samples"
    },
    "metadata": {
      "type": "json"
    },
    "disease": {
      "type": "string",
      "default": "None"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "fresh",
        "frozen"
      ]
    }
  }
}
