{
  "kind": "collectionType",
  "collectionName": "studies",
  "info": {
    "singularName": "study",
    "pluralName": "studies",
    "displayName": "Study",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "datafile": {
      "type": "relation",
      "relation": "morphOne",
      "target": "plugin::data-import.datafile",
      "morphBy": "related",
      "private": true,
      "configurable": false
    },
    "study_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "publications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::publication.publication"
    },
    "experiments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experiment.experiment",
      "mappedBy": "study"
    },
    "datasets": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::dataset.dataset",
      "mappedBy": "studies"
    },
    "analyses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::analysis.analysis"
    },
    "uid": {
      "type": "uid",
      "targetField": "study_id",
      "required": true
    }
  }
}
