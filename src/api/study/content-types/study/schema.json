{
  "kind": "collectionType",
  "collectionName": "studies",
  "info": {
    "singularName": "study",
    "pluralName": "studies",
    "displayName": "Study",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "datafile": {
      "type": "relation",
      "relation": "morphOne",
      "target": "plugin::data-import.datafile",
      "morphBy": "related",
      "private": true,
      "configurable": false
    },
    "study_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "publications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::publication.publication"
    },
    "uid": {
      "type": "uid",
      "targetField": "study_id",
      "required": true
    },
    "datasets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dataset.dataset",
      "mappedBy": "study"
    },
    "disease_datasets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::disease-dataset.disease-dataset"
    },
    "contributors": {
      "type": "component",
      "repeatable": true,
      "component": "contributions.people"
    },
    "teams": {
      "type": "component",
      "repeatable": true,
      "component": "contributions.team"
    },
    "subtitle": {
      "type": "text"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "password": {
      "type": "customField",
      "customField": "plugin::custom-fields.customPassword",
      "private": true
    },
    "resources": {
      "type": "component",
      "repeatable": true,
      "component": "data.resource"
    }
  }
}
