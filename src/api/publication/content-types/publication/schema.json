{
  "kind": "collectionType",
  "collectionName": "publications",
  "info": {
    "singularName": "publication",
    "pluralName": "publications",
    "displayName": "Publication",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "is_published": {
      "type": "boolean",
      "default": false
    },
    "date": {
      "type": "date"
    },
    "abstract": {
      "type": "text"
    },
    "doi": {
      "type": "string"
    },
    "journal": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::journal.journal"
    },
    "volume": {
      "type": "string"
    },
    "issue": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::person.person"
    },
    "datafile": {
      "type": "relation",
      "relation": "morphOne",
      "target": "plugin::data-import.datafile",
      "morphBy": "related",
      "private": true,
      "configurable": false
    },
    "uid": {
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}
